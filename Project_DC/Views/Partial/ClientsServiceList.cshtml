@model IEnumerable<Project_DC.Models.ClientsService>

    <h4>Clients Service</h4>
    <p>
        <a asp-controller="ClientsService" asp-action="Create">Create New</a>
    </p>
    <table class="table">
        <thead>
            <tr>
                <th>
                    @Html.DisplayNameFor(model => model.ServiceDate)
                </th>
                <th>
                    @Html.DisplayNameFor(model => model.StaffsId)
                </th>
                <th></th>
            </tr>
        </thead>
        <tbody>
            @foreach (var item in Model)
            {
                <tr>
                    <td>
                        @Html.DisplayFor(modelItem => item.ServiceDate)
                    </td>
                    <td>
                        @Html.DisplayFor(modelItem => item._Staffs.FullName)
                    </td>
                    <td>
                        <a asp-controller="ClientsService" asp-action="Edit" asp-route-id="@item.Id">Edit</a> |
                        <a asp-controller="ClientsService" asp-action="Details" asp-route-id="@item.Id">Details</a> |
                        <a asp-controller="ClientsService" asp-action="Delete" asp-route-id="@item.Id">Delete</a>
                    </td>
                </tr>
            }
        </tbody>
    </table>
