@model IEnumerable<Project_DC.Models.ClientsTooth>

<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.ToothId)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.ToothStateId)
            </th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item._Tooth.ToothDetails)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item._ToothState.ToothStateName)
                </td>
            </tr>
        }
    </tbody>
</table>
