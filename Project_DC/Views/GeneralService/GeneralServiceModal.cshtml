@model Project_DC.Models.GeneralService
 <div class="row">
    <div class="col-md-7">
        <div id="ValidationSummary" class="text-danger"></div>
        <input id="GeneralService_Id" type="hidden" asp-for="Id" />
        <input type="hidden" asp-for="ClientsServiceId" />
        <div class="form-group">
            <label asp-for="DentalServiceId" class="control-label"></label>
            <select id="GeneralService_DentalServiceId" asp-for="DentalServiceId" class="form-control" asp-items="ViewBag.DentalServiceId"></select>
        </div>
        <div class="form-group">
            <label asp-for="Comment" class="control-label"></label>
            <input id="GeneralService_Comment" asp-for="Comment" class="form-control" />
        </div>
        <div class="form-group">
            <label asp-for="Price" class="control-label"></label>
            <input id="GeneralService_Price" asp-for="Price" class="form-control" />
        </div>
    </div>
</div>
@section Scripts {
    @{await Html.RenderPartialAsync("_ValidationScriptsPartial");}
}

